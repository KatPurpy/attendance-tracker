@model object


<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			<form id="inputForm_edit">
				<div class="modal-header">
					<h5 class="modal-title" id="exampleModalLongTitle">Edit @ViewData["ObjectType"]</h5>
					<button type="button" class="close" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</button>
				</div>
				<div class="modal-body">
					@Html.EditorFor((e) => e)
				</div>
				<div class="modal-footer">
					<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
					<button type="button" onclick="ChangeCrudObj('inputForm_edit','@ViewData["ObjectType"]')" class="btn btn-primary">Save changes</button>
				</div>
			</form>
		</div>
	</div>
</div>